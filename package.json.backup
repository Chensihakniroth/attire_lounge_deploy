{
  "name": "attire-lounge",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "tw:init": "node node_modules/tailwindcss/lib/cli.js init -p",
    "tw:build": "node node_modules/tailwindcss/lib/cli.js -i ./resources/css/app.css -o ./public/css/app.css",
    "tw:watch": "node node_modules/tailwindcss/lib/cli.js -i ./resources/css/app.css -o ./public/css/app.css --watch"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.7",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.309.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tailwindcss": "^3.3.6"
  }
}
